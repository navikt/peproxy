{
  "apiVersion": "nais.io/v1alpha1",
  "kind": "Application",
  "metadata": {
    "name": "peproxy",
    "namespace": "default",
    "labels": {
      "team": "teampeon"
    }
  },
  "spec": {
    "image": "navikt/peproxy:latest",
    "team": "teampeon",
    "port": 8080,
    "ingresses": [
      "https://peproxy.nais.preprod.local"
    ],
    "liveness": {
      "path": "internal/isAlive",
      "initialDelay": 35,
      "periodSeconds": 1,
      "failureThreshold": 60
    },
    "readiness": {
      "path": "internal/isReady",
      "initialDelay": 30,
      "periodSeconds": 1,
      "failureThreshold": 60
    },
    "replicas": {
      "min": 1,
      "max": 1
    },
    "prometheus": {
      "enabled": true,
      "path": "internal/metrics"
    },
    "resources": {
      "limits": {
        "cpu": "1000m",
        "memory": "512Mi"
      },
      "requests": {
        "cpu": "500m",
        "memory": "512Mi"
      }
    },
    "env": [
      {
        "name": "SPRING_PROFILES_ACTIVE",
        "value": "nav"
      }
    ]
  }
}
